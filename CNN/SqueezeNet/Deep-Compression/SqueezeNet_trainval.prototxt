}
layer {
  name: "fire3/concat"
  type: "Concat"
  bottom: "fire3/conv1x1_2"
  bottom: "fire3/conv3x3_2"
  top: "fire3/concat"
}
layer {
  name: "fire4/conv1x1_1"
  type: "Convolution"
  bottom: "fire3/concat"
  top: "fire4/conv1x1_1"
  convolution_param {
    num_output: 32
    kernel_size: 1
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "fire4/relu_conv1x1_1"
  type: "ReLU"
  bottom: "fire4/conv1x1_1"
  top: "fire4/conv1x1_1"
}
layer {
  name: "fire4/conv1x1_2"
  type: "Convolution"
  bottom: "fire4/conv1x1_1"
  top: "fire4/conv1x1_2"
  convolution_param {
    num_output: 128
    kernel_size: 1
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "fire4/relu_conv1x1_2"
  type: "ReLU"
  bottom: "fire4/conv1x1_2"
  top: "fire4/conv1x1_2"
}
layer {
  name: "fire4/conv3x3_2"
  type: "Convolution"
  bottom: "fire4/conv1x1_1"
  top: "fire4/conv3x3_2"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "fire4/relu_conv3x3_2"
  type: "ReLU"
  bottom: "fire4/conv3x3_2"
  top: "fire4/conv3x3_2"
}
layer {
  name: "fire4/concat"
  type: "Concat"
  bottom: "fire4/conv1x1_2"
  bottom: "fire4/conv3x3_2"
  top: "fire4/concat"
}
layer {
  name: "pool4"
  type: "Pooling"
  bottom: "fire4/concat"
  top: "pool4"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "fire5/conv1x1_1"
  type: "Convolution"
  bottom: "pool4"
  top: "fire5/conv1x1_1"
  convolution_param {
    num_output: 32
    kernel_size: 1
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "fire5/relu_conv1x1_1"
  type: "ReLU"
  bottom: "fire5/conv1x1_1"
  top: "fire5/conv1x1_1"
}
layer {
  name: "fire5/conv1x1_2"
  type: "Convolution"
  bottom: "fire5/conv1x1_1"
  top: "fire5/conv1x1_2"
  convolution_param {
